{
    "overrides": [
        {
            "files": ["*.yml", "*.yaml", "*.md"],
            "options": {
                "tabWidth": 2
            }
        }
    ],
    "plugins": ["prettier-plugin-astro"],
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "useTabs": false
}
